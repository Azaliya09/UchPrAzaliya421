<Page x:Class="UchAzaliya.Pages.ForUser"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UchAzaliya.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ForUser" Background="#FFFEF9DE">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button   x:Name="ListEmpl" Width="150" Height="30" Click="ListEmpl_Click">Учет работников</Button>
            <Button x:Name="MatComp" Width="150" Height="50" Click="MatComp_Click">
                <TextBlock Text="Учет материалов и комплектующих" TextAlignment="Center" TextWrapping="Wrap" />
            </Button>

        </StackPanel>
    </Grid>
</Page>
